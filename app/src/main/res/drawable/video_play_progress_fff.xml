        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="#ffffff"/>
                <size android:height="10.0dip"/>
                <corners android:radius="10.0dip"/>
            </shape>
        </clip>
    </item>
</layer-list>
